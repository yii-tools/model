{
    "name": "yii-tools/model",
    "type": "library",
    "description": "Filament model for Yii 3",
    "keywords": [
        "yii",
        "filament",
        "model"
    ],
    "license": "mit",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "yiisoft/arrays": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/infection-static-analysis-plugin": "^1.28.0",
        "vimeo/psalm": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Yii\\Model\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yii\\Model\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
